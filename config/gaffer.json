{
  "launcherType": "bin",
  "config": {
    "executable": "$( locres apps/gaffer/$UVER_GAFFER_VERSION )/bin/$UCORE_OS/bin/gaffer"
  },
  "addons": {
    "shotgun": {
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$( locres libs/shotgun/$UVER_SHOTGUN_VERSION )/lib/python2"
          ]
        }
      }
    },
    "ushotgun": {
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$( locres libs/ushotgun/$UVER_USHOTGUN_VERSION )/lib/python$(majorver $UVER_UPYTHON_VERSION)/lib"
          ]
        }
      }
    }
  }
}
