{
  "launcherType": "bin",
  "config": {
    "executable": "$( locres apps/python/$UVER_UPYTHON_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/bin/python$(majorminorver $UVER_UPYTHON_VERSION)"
  },
  "addons": {
    "pyside2": {
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$( locres libs/pyside2/$UVER_PYSIDE2_VERSION )/$UCORE_OS"
          ],
          "LD_LIBRARY_PATH": [
            "$( locres libs/qt/$UVER_QT_VERSION )/$UCORE_OS/$(majorminorver $UVER_QT_VERSION)/gcc_64/lib"
          ]
        },
        "override": {
          "QT_QPA_PLATFORM_PLUGIN_PATH": [
            "$( locres libs/qt/$UVER_QT_VERSION )/$UCORE_OS/$(majorminorver $UVER_QT_VERSION)/gcc_64/plugins"
          ]
        }
      }
    },
    "setuptools": {
      "env": {
        "prepend": {
          "PYTHONPATH": [
            "$( locres libs/setuptools/$UVER_SETUPTOOLS_VERSION )/lib/python$(majorminorver $UVER_UPYTHON_VERSION)/site-packages"
          ]
        }
      }
    },
    "openimageio": {
      "env": {
        "prepend": {
          "PYTHONPATH": "$( locres libs/openimageio/$UVER_OPENIMAGEIO_VERSION )/$UCORE_OS/lib/python$(majorver $UVER_UPYTHON_VERSION)/site-packages",
          "LD_LIBRARY_PATH": [
            "$( locres libs/png/$UVER_PNG_VERSION )/$UCORE_OS/lib",
            "$( locres libs/openimageio/$UVER_OPENIMAGEIO_VERSION )/$UCORE_OS/lib"
          ]
        }
      }
    },
    "opencolorio": {
      "env": {
        "prepend": {
          "PYTHONPATH": "$( locres libs/opencolorio/$UVER_OPENCOLORIO_VERSION )/$UCORE_OS/lib/python$(majorminorver $UVER_UPYTHON_VERSION)/site-packages",
          "LD_LIBRARY_PATH": [
            "$( locres libs/opencolorio/$UVER_OPENCOLORIO_VERSION )/$UCORE_OS/lib"
          ]
        }
      }
    },
    "tank": {
      "env": {
        "prepend": {
          "PYTHONPATH": "$( locres libs/tank/$UVER_TANK_VERSION )/lib/python$(majorminorver $UVER_UPYTHON_VERSION)"
        }
      }
    }
  }
}
