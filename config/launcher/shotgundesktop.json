{
    "launcherType": "bin",
    "config": {
        "executable": "$( locres apps/shotgunDesktop/$UVER_SHOTGUNDESKTOP_VERSION )/bin/$UCORE_OS/$UCORE_OS_VERSION/Shotgun"
    },
    "env": {
        "unset": [
            "QT_PLUGIN_PATH"
        ]
    },
    "addons": {
        "ingestor":{
            "env": {
                "prepend": {
                    "PYTHONPATH": [
                        "$( locres apps/ingestor/$UVER_INGESTOR_VERSION )/python$(majorver $UVER_UPYTHON_VERSION)/lib"
                    ]
                }
            }
        },
        "centipede":{
            "env": {
                "prepend": {
                    "PYTHONPATH": [
                        "$( locres apps/centipede/$UVER_CENTIPEDE_VERSION )/python$(majorver $UVER_UPYTHON_VERSION)/lib"
                    ]
                }
            }
        }
    }
}
